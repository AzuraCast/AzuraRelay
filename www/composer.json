{
  "name": "azuracast/azurarelay",
  "description": "A companion relay tool to the AzuraCast web radio management suite.",
  "homepage": "https://github.com/AzuraCast/AzuraRelay",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "ext-xmlwriter": "*",
    "azuracast/nowplaying": "^1.1",
    "azuracast/php-api-client": "^2.0",
    "cakephp/chronos": "^1.2",
    "composer/ca-bundle": "^1.2",
    "guzzlehttp/guzzle": ">7.0",
    "http-interop/http-factory-guzzle": "^1.0",
    "laminas/laminas-config": "^3.3",
    "lstrojny/fxmlrpc": "^0.15.0",
    "mnapoli/silly-php-di": "^1.2",
    "monolog/monolog": "^2",
    "php-di/php-di": "^6.0",
    "php-di/slim-bridge": "^3.0",
    "php-http/guzzle7-adapter": "^0.1.1",
    "php-http/httplug": "^2.0",
    "php-http/message": "^1.4",
    "php-http/socket-client": "^2.0",
    "psr/http-factory": "^1.0",
    "slim/http": "^1.2",
    "slim/slim": "^4.2",
    "supervisorphp/supervisor": "^3.0",
    "symfony/console": "^4.3",
    "symfony/event-dispatcher": "^4.3"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.11.1",
    "phpstan/phpstan-strict-rules": "^0.11.0",
    "roave/security-advisories": "dev-master",
    "overtrue/phplint": "^1.1"
  },
  "scripts": {
    "test": [
      "@composer install",
      "@phplint",
      "@phpstan"
    ],
    "phplint": "phplint",
    "phpstan": "phpstan analyze"
  },
  "authors": [
    {
      "name": "Buster Neece",
      "email": "buster@busterneece.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "src"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
